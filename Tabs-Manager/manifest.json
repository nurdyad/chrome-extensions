{

  "manifest_version": 3,
  
  "name": "Link & Tab Manager",
  
  "version": "4.0",
  
  "description": "Open document links or close unselected tabs.",
  
  "permissions": [
  
  "tabs",
  
  "activeTab",
  
  "scripting",
  
  "windows",
  
  "tabGroups",
  
  "system.display"
  
  ],
  
  "host_permissions": [
  
  "<all_urls>"
  
  ],
  
  "action": {
  
  "default_popup": "popup.html",
  
  "default_icon": {
  
  "16": "icons/icon-16.png",
  
  "48": "icons/icon-48.png",
  
  "128": "icons/icon-128.png"
  
  },
  
  "default_title": "Link & Tab Manager"
  
  },
  
  "background": {
  
  "service_worker": "background.js"
  
  },
  
  "icons": {
  
  "16": "icons/icon-16.png",
  
  "48": "icons/icon-48.png",
  
  "128": "icons/icon-128.png"
  
  }
  
  }